package firstHw;

public class EvenNumbers {
 
	
	public static void main(String[] args) {
		
		int numbers = 100;
		
		System.out.println("Printing even numbers between 1 and 100:" +numbers  );
		
		for(int i=1; i<=100; i++)
		{
			 if(i%2==0)
			 {
				 System.out.println(i+"");
			 }
			 
		}
	}
}
